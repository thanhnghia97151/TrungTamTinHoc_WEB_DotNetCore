<link href="~/css/jquery-ui.css" rel="stylesheet" />
<form>
    <input type="text" name="q" placeholder="Search Here" />
    <button>Search</button>
</form>
<script src="~/js/jquery-ui.js"></script>
<script>
    $('input[name="q"]').autocomplete({
        source: '/dashboard/excel/AutocompleteJson',
        minLength: 3,
        select: function (evt, ui) {
            console.log(ui);
            $.post(`/dashboard/excel/getproducts/${ui.item.id}`, (d) => {
                for (var i in d) {
                    $(sheet).append(
                        `
                        <tr>
                            <td>${d[i]['rowId']}</td>
                            <td>${d[i]['orderId']}</td>
                            <td>${d[i]['orderDate']}</td>
                            <td>${d[i]['customerId']}</td>
                            <td>${d[i]['customerName']}</td>
                            <td>${d[i]['segment']}</td>
                            <td>${d[i]['country']}</td>
                            <td>${d[i]['state']}</td>
                            <td>${d[i]['postalCode']}</td>
                            <td>${d[i]['region']}</td>
                            <td>${d[i]['productId']}</td>
                            <td>${d[i]['category']}</td>
                            <td>${d[i]['subCategory']}</td>
                            <td>${d[i]['productName']}</td>
                            <td>${d[i]['sales']}</td>
                            <td>${d[i]['quantity']}</td>
                            <td>${d[i]['discount']}</td>
                            <td>${d[i]['profit']}</td>

                        </tr>
                    `
                    );
                }
            });
        }
    });
</script>
<style>
    body{
        font-family: Arial, Helvetica, sans-serif
    }
    table{
        font-size:1em;
    }
    .ui-draggable, .ui-droppable{
        background-position:top;
    }
</style>
<table class="table table-bordered">
    <thead class="table-dark">
        <tr>
            <th>Row Id</th>
            <th>Order Id</th>
            <th>Order Date</th>
            <th>Ship Date</th>
            <th>Customer Id</th>
            <th>Customer Name</th>
            <th>Segment</th>
            <th>Coutry</th>
            <th>State</th>
            <th>Postal Code</th>
            <th>Region</th>
            <th>Product Id</th>
            <th>Category</th>
            <th>Sub Category</th>
            <th>Product name</th>
            <th>Sales</th>
            <th>Quantity</th>
            <th>Discount</th>
            <th>Profit</th>
        </tr>
    </thead>
    <tbody id="sheet">

    </tbody>
</table>