@model List<WebApp1.Models.Access>
<a href="/dashboard/access/create" class="btn btn-primary">Create</a>
<form action="/dashboard/access/deletemany" method="post">
    <table class="table table-bordered">
        <thead class="table-dark">
            <tr>
                <th>
                    <button>Delete</button>
                </th>
                <th>Id</th>
                <th>Name</th>
                <th>Role</th>
                <th>Url</th>
                <th>Edit</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        <input type="checkbox" name="ids" value="@item.Id"/>
                    </td>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.Role.Name</td>
                    <td>@item.Url</td>
                    <td>
                        <a href="/dashboard/access/edit/@item.Id">
                            <i class="fa fa-edit"></i>
                        </a>
                    </td>
                </tr>
            }
        </tbody>
    </table>

</form>

